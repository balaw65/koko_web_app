<% provide(:title, "Set Password") %>
<h1 class="ml-1">Set Password</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:password_reset, url: password_resets_path) do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email, value:"", class: 'form-control' %><br>
      <%= f.submit "Set Password", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

